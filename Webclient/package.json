{
  "name": "webclient",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:mac": "ng serve --proxy-config proxy.conf.json --port 80",
    "start:windows": "ng serve --proxy-config proxy.conf.json --port 80",
    "prebuild": "bash tools/prepare.sh",
    "prebuild:mac": "bash tools/prepare.sh",
    "prestart": "bash tools/prepare.sh",
    "prestart:mac": "bash tools/prepare.sh",
    "pretest": "bash tools/prepare.sh",
    "pretest:mac": "bash tools/prepare.sh",
    "prelint": "bash tools/prepare.sh",
    "prelint:mac": "bash tools/prepare.sh",
    "build": "ng build --prod --configuration=production --build-optimizer",
    "postbuild": "bash tools/minifyHtml.sh && bash tools/inlineGlobalStyles.sh",
    "test": "jest",
    "lint": "ng lint",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.4",
    "@angular/common": "^10.0.4",
    "@angular/compiler": "^10.0.4",
    "@angular/core": "^10.0.4",
    "@angular/forms": "^10.0.4",
    "@angular/platform-browser": "^10.0.4",
    "@angular/platform-browser-dynamic": "^10.0.4",
    "@angular/router": "^10.0.4",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "rxjs": "^6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.3",
    "@angular/cli": "^10.0.3",
    "@angular/compiler-cli": "^10.0.4",
    "@angular/language-service": "^10.0.4",
    "@types/jest": "^24.0.22",
    "@types/node": "^12.12.50",
    "codelyzer": "^6.0.0",
    "html-minifier": "^4.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^24.9.0",
    "jest-preset-angular": "^8.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7"
  }
}
