{
    "name": "webclient",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start:mac": "ng serve --proxy-config proxy.conf.json --port 80",
        "start:windows": "ng serve --proxy-config proxy.conf.json --port 80",
        "prebuild": "bash tools/prepare.sh",
        "prebuild:mac": "bash tools/prepare.sh",
        "prestart": "bash tools/prepare.sh",
        "prestart:mac": "bash tools/prepare.sh",
        "pretest": "bash tools/prepare.sh",
        "pretest:mac": "bash tools/prepare.sh",
        "prelint": "bash tools/prepare.sh",
        "prelint:mac": "bash tools/prepare.sh",
        "build": "ng build --prod --configuration=production --build-optimizer",
        "postbuild": "bash tools/minifyHtml.sh && bash tools/inlineGlobalStyles.sh",
        "test": "jest",
        "lint": "ng lint"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^9.0.2",
        "@angular/common": "^9.0.2",
        "@angular/compiler": "^9.0.2",
        "@angular/core": "^9.0.2",
        "@angular/forms": "^9.0.2",
        "@angular/platform-browser": "^9.0.2",
        "@angular/platform-browser-dynamic": "^9.0.2",
        "@angular/router": "^9.0.2",
        "@ngx-translate/core": "^11.0.1",
        "@ngx-translate/http-loader": "^4.0.0",
        "@wishtack/reactive-component-loader": "^1.1.1",
        "rxjs": "~6.5.4",
        "tslib": "^1.10.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.900.3",
        "@angular/cli": "^9.0.3",
        "@angular/compiler-cli": "^9.0.2",
        "@angular/language-service": "^9.0.2",
        "@types/jest": "^24.0.22",
        "@types/node": "^12.11.1",
        "codelyzer": "^5.1.2",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "jest": "^24.9.0",
        "jest-preset-angular": "^8.0.0",
        "ts-node": "~7.0.0",
        "tslint": "~5.15.0",
        "typescript": "~3.7.5",
        "html-minifier": "^4.0.0"
    }
}
